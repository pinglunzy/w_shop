/*列表刷新*/
.top-refresh {
  overflow: hidden;
  height: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  transition: height 0.5s;
}
.top-refresh.open {
  height: 96rpx;
}
.top-refresh .circle {
  width: 30rpx;
  height: 30rpx;
  margin: 0 10rpx;
  background: #CCCCCC;
  border-radius: 50%;
  transform: scale(0);
  animation: bulge 2s infinite ease-in-out;
}
@keyframes bulge {
50% {
    transform: scale(1);
}
}
.top-refresh .circle:nth-child(1) {
  animation-delay: 0s;
}
.top-refresh .circle:nth-child(2) {
  animation-delay: 0.25s;
}
.top-refresh .circle:nth-child(3) {
  animation-delay: 0.5s;
}
/*foot-btns*/
.foot-btns {
