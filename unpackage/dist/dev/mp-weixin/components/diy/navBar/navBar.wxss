.diy-navbar {
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.diy-navbar .item {
  display: flex;
  box-sizing: border-box;
  padding: 20rpx;
  box-sizing: border-box;
  box-shadow: 0px 3rpx 6rpx rgba(0, 0, 0, 0.16);
  border-radius: 18rpx;
}
.diy-navbar .item.rowsNum1 {
  justify-content: flex-start;
  align-items: flex-start;
  width: 690rpx;
  height: 150rpx;
}
.diy-navbar .item.rowsNum2 {
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 330rpx;
  height: 254rpx;
}
.diy-navbar .item image {
  width: 100rpx;
  height: 100rpx;
}
.diy-navbar .item text {
  display: block;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 100%;
  overflow: hidden;
  font-size: 22rpx;
}
.diy-navbar .item .title {
  font-weight: 600;
  font-size: 30rpx;
  color: #333333;
}
.diy-navbar .item .icon-jiantou {
  font-size: 26rpx;
  margin-left: 12rpx;
}
[data-theme=theme0] .diy-navbar .item .icon-jiantou {
  color: #FFCC00 !important;
}
[data-theme=red_theme] .diy-navbar .item .icon-jiantou {
  color: #e60b30 !important;
}
[data-theme=theme1] .diy-navbar .item .icon-jiantou {
  color: #8f2df3 !important;
}
[data-theme=theme2] .diy-navbar .item .icon-jiantou {
  color: #33a7ff !important;
}
[data-theme=theme3] .diy-navbar .item .icon-jiantou {
  color: #1fc551 !important;
}
[data-theme=theme4] .diy-navbar .item .icon-jiantou {
  color: #ff7f00 !important;
}
[data-theme=theme5] .diy-navbar .item .icon-jiantou {
  color: #caa45a !important;
}
.nav-image {
  width: 100rpx;
  height: 100rpx;
}
.nav-image image {
  width: 100rpx;
  height: 100rpx;
}
.item.rowsNum2 .nav-image {
  margin-bottom: 18rpx;
}