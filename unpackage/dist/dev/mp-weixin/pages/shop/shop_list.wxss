.info {
  margin-left: 20rpx;
}
.address-list {
  padding: 30rpx 0;
}
.address-list .address {
  margin-bottom: 22rpx;
}
.address-list .address .info-right {
  height: 100rpx;
  border-top: 2rpx solid #EEEEEE;
  flex-shrink: 0;
}
.foot-btns {
  padding: 0;
}
.foot-btns .btn-red {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 0;
}
.address_img image {
  width: 154rpx;
  height: 154rpx;
  border-radius: 15rpx;
}
.address_tit {
  height: 75rpx;
  line-height: 75rpx;
  color: #959597;
  font-size: 29rpx;
  padding-left: 28rpx;
}
.address_btn {
  width: 130rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
  text-align: center;
  margin-top: 50rpx;
  color: #FFFFFF;
}
[data-theme=theme0] .address_btn {
  background-color: #FFCC00 !important;
}
[data-theme=red_theme] .address_btn {
  background-color: #e60b30 !important;
}
[data-theme=theme1] .address_btn {
  background-color: #8f2df3 !important;
}
[data-theme=theme2] .address_btn {
  background-color: #33a7ff !important;
}
[data-theme=theme3] .address_btn {
  background-color: #1fc551 !important;
}
[data-theme=theme4] .address_btn {
  background-color: #ff7f00 !important;
}
[data-theme=theme5] .address_btn {
  background-color: #caa45a !important;
}
